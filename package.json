{
  "name": "acquit-markdown",
  "version": "0.0.5",
  "description": "Quick tool for converting acquit output to markdown",
  "main": "index.js",
  "bin": {
    "acquit-markdown": "./bin/acquit-markdown"
  },
  "scripts": {
    "docs": "node docs.js",
    "test": "NODE_PATH=../ mocha --harmony test/*",
    "test-coverage": "NODE_PATH=../ istanbul cover _mocha -- --harmony -R spec ./test/*"
  },
  "author": "Valeri Karpov <val@karpov.io>",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/vkarpov15/acquit-markdown.git"
  },
  "dependencies": {
    "commander": "2.13.0"
  },
  "peerDependencies": {
    "acquit": ">=0.4.0"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requirePaddingNewLinesAfterBlocks": null,
    "requireMultipleVarDecl": null,
    "requireTrailingComma": null
  },
  "devDependencies": {
    "acquit": "0.5.1",
    "istanbul": "0.3.5",
    "mocha": "2.0.1"
  }
}
